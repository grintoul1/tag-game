	end

Route113_EventScript_Tia::
	msgbox Route113_Text_TiaPostBattle, MSGBOX_AUTOCLOSE
	end

Route113_EventScript_Sophie::
	msgbox Route113_Text_SophiePostBattle, MSGBOX_AUTOCLOSE
	end

